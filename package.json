{
  "name": "used-car-pr-app",
  "version": "1.0.0",
  "description": "A simple CAP project.",
  "repository": "<Add your repository here>",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@cap-js/hana": "^2",
    "@sap/cds": "^9",
    "@sap/xssec": "^4",
    "cds-launchpad-plugin": "^2.2.0",
    "express": "^4"
  },
  "engines": {
    "node": ">=20"
  },
  "devDependencies": {
    "@cap-js/cds-types": "^0.13.0",
    "@cap-js/sqlite": "^2",
    "@sap/cds-dk": ">=8",
    "mbt": "^1.2.29",
    "rimraf": "^5.0.5"
  },
  "scripts": {
    "start": "cds-serve",
    "undeploy": "cf undeploy used-car-pr-app --delete-services --delete-service-keys --delete-service-brokers",
    "build": "rimraf resources mta_archives && mbt build --mtar archive",
    "deploy": "cf deploy mta_archives/archive.mtar --retries 1",
    "watch-assignment-1": "cds watch --open assignment-1/webapp/index.html?sap-ui-xx-viewCache=false",
    "watch-assignment-2": "cds watch --open assignment-2/webapp/index.html?sap-ui-xx-viewCache=false",
    "watch-assignment-3": "cds watch --open assignment-3/webapp/index.html?sap-ui-xx-viewCache=false",
    "watch-assignment-4": "cds watch --open assignment-4/webapp/index.html?sap-ui-xx-viewCache=false",
    "watch-assignment-5": "cds watch --open assignment-5/webapp/index.html?sap-ui-xx-viewCache=false",
    "watch-assignment-6": "cds watch --open assignment-6/webapp/index.html?sap-ui-xx-viewCache=false",
    "watch-freestyle": "cds watch --open freestyle/webapp/index.html?sap-ui-xx-viewCache=false"
  },
  "cds": {
    "requires": {
      "[production]": {
        "auth": "xsuaa"
      },
      "connectivity": true,
      "html5-runtime": true
    },
    "launchpad": {
      "version": "1.132.0"
    }
  },
  "sapux": [
    "app/assignment-1",
    "app/assignment-2",
    "app/assignment-3",
    "app/assignment-4",
    "app/assignment-5",
    "app/assignment-6"
  ]
}
